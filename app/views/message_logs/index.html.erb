<p id="notice"><%= notice %></p>

<h1>Message Logs</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>To</th>
      <th>From</th>
      <th>Status</th>
      <th>Sid</th>
      <th>Error code</th>
      <th>Error message</th>
      <th>Date sent</th>
      <th>Account sid</th>
      <th>Billing reference</th>
      <th>Message</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @message_logs.each do |message_log| %>
      <tr>
        <td><%= message_log.to %></td>
        <td><%= message_log.from %></td>
        <td><%= message_log.status %></td>
        <td><%= message_log.sid %></td>
        <td><%= message_log.error_code %></td>
        <td><%= message_log.error_message %></td>
        <td><%= message_log.date_sent %></td>
        <td><%= message_log.account_sid %></td>
        <td><%= message_log.billing_reference %></td>
        <td><%= message_log.message %></td>
        <td><%= link_to 'Show', message_log %></td>
        <td><%= link_to 'Destroy', message_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
